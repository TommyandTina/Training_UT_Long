        "table": [
            {
                "no": "#376400",
                "description": "EtherAVB: Missing NULL check for pointer type of argument",
                "impact": "User may not set NULL pointer for argument. In normal usage, no impact for user.",
                "note": "work_around: None <BR>It will be resolved by SDK v3.11.0."
            },
            {
                "no": "#376402",
                "description": "EtherAVB: Can not call eth_rcar_start_device after eth_rcar_stop_device",
                "impact": "User may not call eth_rcar_start_device twice.",
                "note": "work_around: None <BR>It will be resolved by SDK v3.11.0."
            },
            {
                "no": "#376409",
                "description": "EtherAVB: When define EtherAVB1 or  AVB2 individually in device tree, port initialization for each AVB channel will not work as expected",
                "impact": "User may not define EtherAVB1 or AVB2 individually in device tree.",
                "note": "work_around: Define EtherAVB0, AVB1, AVB2 continuously in device tree. <BR>It will be resolved by SDK v3.11.0. "
            }
        ]
