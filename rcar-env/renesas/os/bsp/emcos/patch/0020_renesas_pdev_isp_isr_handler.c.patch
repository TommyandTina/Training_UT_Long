+/****************************************************************************
+ISR handler function for GIC problem workaround
+****************************************************************************/
+#include "renesas_isr_common.h"
+
+/* refer to bios_ep/chip/rcar4/bios/pdev_rcar4_v4h.c for interrupt function registered in pdev. */
+void rcar_pdev_isp1_isr_handler(uint16_t iid, uintptr_t param)
+{
+    int32_t ret = MCOS_EOK;
+
+    switch(iid)
+    {
+        case RCAR4_INTNO_ISP_0: /* refer to include/bios_ep/chip/rcar4/rcar4_v4h.h for definition */
+            /* Disable ISP_0(interrupt No: 475 is CISP0) */
+
+            /* Disable interrupt by ISP Core Interrput Enable register */
+            *((volatile uint32_t*)(0xffffffff00000000 + 0xfec00000 + 0x80004)) = 0x00000000;
+            break;
+        case RCAR4_INTNO_ISPCS_0:
+            /* Disable ISP_0(interrupt No: 473 is TISP0) */
+
+            /* Disable interrupt by ISP Interrupt enable Register */
+            *((volatile uint32_t*)(0xffffffff00000000 + 0xfed00000 + 0x0060)) = 0x00000000;
+            break;
+        case RCAR4_INTNO_ISPCS_1:
+            /* Disable ISPCS_1(interrupt No: 474 is TISP1) */
+
+            /* Disable interrupt by ISP Interrupt enable Register */
+            *((volatile uint32_t*)(0xffffffff00000000 + 0xfed20000 + 0x0060)) = 0x00000000;
+            break;
+        default:
+            ret = MCOS_ENOEXS; /* refer to include/mcos/mcos.h for error code */
+        break;
+    }
+
+    if (ret != MCOS_EOK)
+    {
+        mcos_debug_printf(
+                "interrupt %d: mcos_interrupt_disable error (%d) in %s/n",
+                iid, ret, __func__);
+    }
+    else
+    {
+        pdev_intentry_t *intentry = (pdev_intentry_t *) param;
+        struct lfs_devgetdone req;
+        req.code = DEV_DEVGETDONE;
+        req.type = GETDONE_NONBLOCK | (intentry->intno << 8);
+        req.ptr = (lfs_t**) intentry->itr_config.param;
+        if (intentry->itr_config.uring != NULL)
+        {
+            ret = uring_interrupt_notify_internal(intentry->itr_config.uring, &req, sizeof(struct lfs_devgetdone), true);
+        }
+        else
+        {
+            ret = mcos_rrmessage_send(intentry->itr_config.tid, DEV_DEVGETDONE, &req, sizeof(req), NULL, NULL, 0);
+        }
+
+        if (ret != MCOS_EOK)
+        {
+            mcos_debug_printf(
+                            "interrupt %d: mcos_rrmessage_send error in %s/n",
+                            iid, __func__);
+        }
+    }
+}
+
