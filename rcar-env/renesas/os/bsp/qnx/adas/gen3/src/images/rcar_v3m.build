ravb_start.sh {
    #!/bin/sh
    waitfor /dev/fs0 1 > /dev/null 2>&1
    if [ $? == 0 ]; then
        ETHADDR=`rcar-uboot-env /dev/fs0 ethaddr offset=0x700000 2> /dev/null`

       if [ $? == 0 ]; then
          ETHADDR=`echo $ETHADDR | sed -e "s/ethaddr=//g;s/://g"`
        else
            ETHADDR=""
        fi
    fi

    if [ -z "$ETHADDR" ]; then
        ETHADDR=`genmac-random -m`
    fi
    io-pkt-v6-hc -d ravb mac=$ETHADDR -p tcpip pkt_typed_mem=below4G,smmu=off
    dhclient -m -lf /dev/shmem/dhclient.leases -pf /dev/shmem/dhclient.pid -nw ravb0
}

